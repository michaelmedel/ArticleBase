<h1 class="display-4">Articles</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>#</th>
            <th>Title</th>
            <th>Body</th>
        </tr>
    </thead>
    <tbody>
        <% articles.forEach(function(article){ %>
            <tr>
                <td><%= article.id %></td>
                <td><%= article.title %></td>
                <td><%= article.body %></td>
                <td><a href="/articles/destroy/<%= article.id %>">Delete</a></td>
                <td><a href="/articles/edit/<%= article.id %>">Edit</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>